{
  "name": "eWeLink-Remote-Test",
  "version": "0.4.0",
  "slug": "ewelink_remote",
  "description": "eWeLink-Remote Add-on supports adding eWeLink-Remote sub-devices R5, R5W, S-Mate, S-Mate2 to Home Assistant as trigger conditions for automation",
  "startup": "application",
  "boot": "auto",
  "arch": [
    "armv7",
    "aarch64",
    "amd64"
  ],
  "init": false,
  "panel_icon": "mdi:signal-variant",
  "panel_title": "eWeLink-Remote",
  "ingress": true,
  "ingress_port": 3031,
  "ingress_stream": true,
  "host_network": true,
  "homeassistant_api": true,
  "hassio_role": "admin",
  "hassio_api": true,
  "services": [
    "mqtt:need"
  ],
  "map": [
    {
      "type": "homeassistant_config",
      "read_only": false,
      "path": "/config"
    },
    {
      "type": "all_addon_configs",
      "read_only": false,
      "path": "/all_addon_configs"
    },
    {
      "type": "share",
      "read_only": false,
      "path": "/addon_share"
    },
    {
      "type": "addon_config",
      "read_only": false,
      "path": "/addon_config"
    }
  ],
  "options": {
    "log_level": "Info",
    "mqtt": {}
  },
  "schema": {
    "log_level": "list(None|Info|Debug)?",
    "mqtt": {
      "server": "str?",
      "username": "str?",
      "password": "str?"
    }
  },
  "image": "ghcr.io/h1355054467/remote-test-{arch}"
}