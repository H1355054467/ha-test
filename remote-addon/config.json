{
  "name": "Remote for iHost test",
  "version": "0.0.1",
  "slug": "ihost_remote_test",
  "description": "Remote for iHost test Add-on",
  "url": "https://github.com/H1355054467/ha-test",
  "startup": "application",
  "boot": "auto",
  "arch": [
    "armv7",
    "aarch64",
    "amd64"
  ],
  "init": false,
  "panel_icon": "mdi:alpha-m-circle",
  "panel_title": "Remote for iHost test",
  "ingress": true,
  "ingress_port": 3031,
  "ingress_stream": true,
  "host_network": true,
  "homeassistant_api": true,
  "hassio_role": "admin",
  "hassio_api": true,
  "services": [
    "mqtt:need"
  ],
  "map": [
    {
      "type": "homeassistant_config",
      "read_only": false,
      "path": "/config"
    }
  ],
  "options": {
    "log_level": "Info",
    "mqtt": {}
  },
  "schema": {
    "log_level": "list(None|Info|Debug)?",
    "mqtt": {
      "server": "str?",
      "username": "str?",
      "password": "str?"
    }
  },
  "image": "ghcr.io/ihost-open-source-project/hassio-ihost-remote-armv7",
  "privileged": [
    "BPF",
    "CHECKPOINT_RESTORE",
    "DAC_READ_SEARCH",
    "IPC_LOCK",
    "NET_ADMIN",
    "NET_RAW",
    "PERFMON",
    "SYS_ADMIN",
    "SYS_MODULE",
    "SYS_NICE",
    "SYS_PTRACE",
    "SYS_RAWIO",
    "SYS_RESOURCE",
    "SYS_TIME"
  ],
  "apparmor": false,
  "uart": true,
  "usb": true,
  "gpio": true,
  "audio": true,
  "host_dbus": true,
  "docker_api": true
}