{
  "name": "Remote for iHost test",
  "version": "1.0.3",
  "slug": "ihost_remote_test",
  "description": "Remote for iHost test Add-on",
  "url": "https://github.com/H1355054467/ha-test",
  "startup": "application",
  "boot": "auto",
  "arch": [
    "armv7",
    "aarch64",
    "amd64"
  ],
  "init": false,
  "panel_icon": "mdi:alpha-m-circle",
  "panel_title": "Remote for iHost test",
  "ingress": true,
  "ingress_port": 3031,
  "ingress_stream": true,
  "host_network": true,
  "homeassistant_api": true,
  "hassio_role": "admin",
  "hassio_api": true,
  "services": [
    "mqtt:need"
  ],
  "map": [
    {
      "type": "homeassistant_config",
      "read_only": false,
      "path": "/config"
    }
  ],
  "options": {
    "log_level": "Info",
    "mqtt": {}
  },
  "schema": {
    "log_level": "list(None|Info|Debug)?",
    "mqtt": {
      "server": "str?",
      "username": "str?",
      "password": "str?"
    }
  },
  "image": "ghcr.io/h1355054467/remote-test-{arch}"
}